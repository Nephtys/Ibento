  </div>
  <div id="content">
    <div id="navigation">
      <ul>
        <li><a href="#">Users</a></li>
        <li><a href="#">Events</a></li>
        <li>Teams</li>
        <li>Tasks</li>
      </ul>
    </div>
    <div id="sidebar">
      
    </div>
    <div id="page">
      <%= yield %>
    </div>
  </div>
  <div id="footer">
    
  </div>

</body>
</html>
