<h1>Listing task_lists</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Event</th>
    <th>Team</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @task_lists.each do |task_list| %>
  <tr>
    <td><%= task_list.title %></td>
    <td><%= task_list.description %></td>
    <td><%= task_list.event_id %></td>
    <td><%= task_list.team_id %></td>
    <td><%= link_to 'Show', task_list %></td>
    <td><%= link_to 'Edit', edit_task_list_path(task_list) %></td>
    <td><%= link_to 'Destroy', task_list, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>



